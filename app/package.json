  "scripts": {
    "gql:download": "apollo service:download --endpoint=http://localhost:8085/graphql schema/graphql-schema.json",
    "gql:generate": "apollo client:codegen --includes **/GqlQueries.ts --target typescript --globalTypesFile=src/__generated__/query-types.ts --localSchemaFile schema/graphql-schema.json",
    "gql": "yarn gql:download && yarn gql:generate",
    "tslint:fix": "tslint --fix --project .",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
